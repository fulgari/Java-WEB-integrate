  <filter>
  	<filter-name>ExceptionHandleFilter</filter-name>
  	<filter-class>com.integrate.ExceptionHandleFilter</filter-class>
  </filter>
  
  <filter-mapping>
  	<filter-name>ExceptionHandleFilter</filter-name>
  	<url-pattern>/ExceptionHandler.jsp</url-pattern>
  </filter-mapping>
  
  
  <filter>
  	<filter-name>privilegeFilter</filter-name>
  	<filter-class>com.integrate.PrivilegeFilter</filter-class>
  	<init-param>
  		<param-name>file</param-name>
  		<param-value>/WEB-INF/privilege.properties</param-value>
  	</init-param>
  </filter>
  
  <filter-mapping>
  	<filter-name>privilegeFilter</filter-name>
  	<url-pattern>*.do</url-pattern>
  </filter-mapping>
  
  <filter>
  	<filter-name>gzipFilter</filter-name>
  	<filter-class>com.integrate.GZipFilter</filter-class>
  </filter>
  
  <filter-mapping>
  	<filter-name>gzipFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  
  <filter>
  	<filter-name>cacheFilter</filter-name>
  	<filter-class>com.integrate.CacheFilter</filter-class>
  	<init-param>
  		<param-name>cacheTime</param-name>
  		<param-value>1000000</param-value>
  	</init-param>
  </filter>
  
  <filter-mapping>
  	<filter-name>cacheFilter</filter-name>
  	<url-pattern>*.jsp</url-pattern>
  	<url-pattern>*.html</url-pattern>
  	<url-pattern>*.do</url-pattern>
  </filter-mapping>
