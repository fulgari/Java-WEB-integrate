<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" metadata-complete="true" version="2.4">
  <display-name>Integrate</display-name>
  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>
		  com.integrate.HelloServlet
  	</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/HelloServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetTimeServlet</servlet-name>
    <servlet-class>com.integrate.GetTimeServlet</servlet-class>
    <init-param>
      <param-name>message</param-name>
      <param-value>welcom1!</param-value>
    </init-param>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>utf-8</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetTimeServlet</servlet-name>
    <url-pattern>/GetTimeServlet</url-pattern>
    <url-pattern>/GetTimeServlet.aspx</url-pattern>
    <url-pattern>/GetTimeServlet.php</url-pattern>
    <url-pattern>/GetTimeServlet.jsp</url-pattern>
    <url-pattern>/GetTimeServlet.asp</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>IdentityServlet</servlet-name>
    <servlet-class>com.integrate.IdentityServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IdentityServlet</servlet-name>
    <url-pattern>/IdentityServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InitParamServlet</servlet-name>
    <servlet-class>com.integrate.InitParamServlet</servlet-class>
    <init-param>
      <param-name>helloween</param-name>
      <param-value>password</param-value>
    </init-param>
    <init-param>
      <param-name>admin</param-name>
      <param-value>admin</param-value>
    </init-param>
    <init-param>
      <param-name>babyface</param-name>
      <param-value>babyface</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitParamServlet</servlet-name>
    <url-pattern>/InitParamServlet</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>upload folder</param-name>
    <param-value>attachment</param-value>
  </context-param>
  <context-param>
    <param-name>allowed file type</param-name>
    <param-value>.gif, .jpg, .bmp</param-value>
  </context-param>
  <servlet>
    <servlet-name>ContextParamServlet</servlet-name>
    <servlet-class>com.integrate.ContextParamServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InjectionServlet</servlet-name>
    <servlet-class>com.integrate.InjectionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InjectionServlet</servlet-name>
    <url-pattern>/InjectionServlet</url-pattern>
  </servlet-mapping>
  <env-entry>
    <env-entry-name>hello</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>Hello, welcome to the JavaEE Resource Injection...</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>i</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value>31</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>persons</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>Helloween, Cobain, Roses, Axl, </env-entry-value>
  </env-entry>
  <servlet>
    <servlet-name>SearchServlet</servlet-name>
    <servlet-class>
		  com.integrate.SearchServlet
  	</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchServlet</servlet-name>
    <url-pattern>/SearchServlet</url-pattern>
  </servlet-mapping>
  <servlet>
